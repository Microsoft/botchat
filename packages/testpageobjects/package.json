{
  "name": "testpageobjects",
  "version": "0.0.0-0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:babel && npm run build:esbuild",
    "build:babel": "babel --out-dir lib/ src/",
    "build:esbuild": "esbuild lib/index.js --bundle --outfile=dist/test-page-objects.js --sourcemap --target=chrome80",
    "prestart": "npm run build",
    "start": "concurrently --kill-others \"npm run start:*\"",
    "start:babel": "npm run build:babel -- --skip-initial-build --watch",
    "start:esbuild": "npm run build:esbuild -- --watch"
  },
  "author": "William Wong (https://github.com/compulim)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "concurrently": "^6.0.2",
    "esbuild": "^0.11.16"
  },
  "dependencies": {
    "@babel/preset-react": "^7.13.13",
    "base64-arraybuffer": "^0.2.0",
    "classnames": "^2.3.1",
    "core-js": "^3.11.2",
    "event-iterator": "^2.0.0",
    "event-target-shim": "^6.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.16.0",
    "p-defer": "4.0.0",
    "simple-update-in": "2.2.0"
  }
}
