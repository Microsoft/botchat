{
  "name": "isomorphic-react",
  "version": "0.0.0-0",
  "main": "dist/react.js",
  "private": true,
  "scripts": {
    "eslint": "eslint src/**/*.js",
    "prepublishOnly": "npm run prepublishOnly:babel && npm run prepublishOnly:webpack",
    "prepublishOnly:babel": "babel src --out-dir lib --verbose",
    "prepublishOnly:webpack": "webpack-cli",
    "start": "npm run start:note && npm run start:babel && npm run start:webpack",
    "start:babel": "npm run prepublishOnly:babel",
    "start:note": "echo \"isomorphic-react\" is not under actively development, running in non-watch mode.",
    "start:webpack": "npm run prepublishOnly:webpack"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "concurrently": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  }
}
