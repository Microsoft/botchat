{
  "name": "testharness2",
  "version": "0.0.0-0",
  "description": "",
  "author": "William Wong (https://github.com/compulim)",
  "license": "MIT",
  "main": "lib/index.js",
  "private": true,
  "browser": {
    "path": false,
    "selenium-webdriver": false
  },
  "dependencies": {
    "abort-controller": "3.0.0",
    "core-js": "3.11.0",
    "event-target-shim": "6.0.2",
    "expect": "25.5.0",
    "lolex": "^6.0.0",
    "math-random": "2.0.1",
    "regenerator-runtime": "0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "babel-plugin-istanbul": "^6.0.0",
    "concurrently": "6.0.2",
    "esbuild": "0.11.13",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "26.6.3",
    "jest-environment-node": "^26.6.2",
    "jest-image-snapshot": "4.4.1",
    "node-dev": "6.7.0",
    "node-fetch": "^2.6.1",
    "p-defer": "^3.0.0",
    "selenium-webdriver": "4.0.0-beta.3",
    "serve": "11.3.2",
    "strip-ansi": "^6.0.0"
  },
  "scripts": {
    "build": "npm run build:babel && npm run build:esbuild",
    "build:babel": "concurrently \"npm run build:babel:*\"",
    "build:babel:cjs": "babel --config-file ./babel.cjs.config.json --out-dir lib src",
    "build:babel:esm": "babel --config-file ./babel.esm.config.json --out-dir lib/esm src",
    "build:esbuild": "esbuild lib/esm/browser/index.js --bundle --define:process.env.CI=undefined --outfile=dist/test-harness2.js --sourcemap --target=chrome80",
    "prestart": "concurrently \"npm run build:babel:*\" && npm run build:esbuild",
    "start": "concurrently --kill-others \"npm run start:*\"",
    "start:babel:cjs": "npm run build:babel:cjs -- --skip-initial-build --watch",
    "start:babel:esm": "npm run build:babel:esm -- --skip-initial-build --watch",
    "start:esbuild": "npm run build:esbuild -- --watch",
    "test": "node node_modules/.bin/jest --maxWorkers=4"
  }
}
