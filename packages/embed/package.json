{
  "name": "embed",
  "version": "0.0.0-0",
  "description": "",
  "main": "src/index.js",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/*.{spec,test}.{js,jsx,ts,tsx}",
      "!**/__tests__/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "eslint": "eslint src/**/*.js --ignore-pattern *.spec.js --ignore-pattern *.test.js --ignore-pattern __tests__",
    "prepublishOnly": "npm run prepublishOnly:babel && npm run prepublishOnly:webpack",
    "prepublishOnly:babel": "babel src --ignore **/*.spec.js,**/*.test.js,__tests__/**/*.js --out-dir lib --verbose",
    "prepublishOnly:webpack": "webpack-cli",
    "prestart": "npm run prepublishOnly:babel",
    "start": "npm run start:babel && concurrently --kill-others --names \"babel,server,webpack\" \"npm run start:babel:watch\" \"npm run start:server\" \"npm run start:webpack\"",
    "start:babel": "cross-env node_env=test npm run prepublishOnly:babel --",
    "start:babel:watch": "cross-env node_env=test npm run start:babel -- --skip-initial-build --watch",
    "start:server": "node-dev --no-notify --respawn hostDevServer",
    "start:webpack": "cross-env node_env=test npm run prepublishOnly:webpack -- --config webpack-development.config --watch",
    "test": "jest --watch",
    "test:ci": "cross-env node_env=test npm run test -- --ci --coverage true --no-watch"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "jest": "^24.9.0",
    "node-dev": "^4.0.0",
    "node-fetch": "^2.6.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "restify": "^8.5.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "core-js": "^3.5.0",
    "url-search-params-polyfill": "^7.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
